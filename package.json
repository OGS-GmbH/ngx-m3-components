{
	"name": "@ogs-gmbh/ngx-m3-components",
	"version": "2.0.0",
	"description": "A collection of reusable, customizable UI components built with Angular and Material 3.",
	"keywords": [
		"angular",
		"material",
		"material3",
		"angular-material",
		"custom-components",
		"ui-library"
	],
	"homepage": "https://ogs-gmbh.github.io/ngx-m3-components",
	"bugs": "https://github.com/OGS-GmbH/ngx-m3-components/issues",
	"license": "MIT",
	"author": "OGS GmbH",
	"contributors": [
		{
			"name": "Simon Kovtyk",
			"email": "simon@kovtyk.com",
			"url": "https://simonkov.dev"
		}
	],
	"exports": {
		".": {
			"sass": "./_public-api.scss"
		}
	},
	"repository": {
    "type": "git",
    "url": "https://github.com/OGS-GmbH/ngx-m3-components"
  },
	"scripts": {
		"ci:build:main:production": "ng build",
		"ci:docs:extract:typedoc": "typedoc",
		"ci:docs:main:vitepress": "bash ./.vitepress/scripts/pre-vitepress.sh && vitepress build .vitepress",
		"ci:pr:commitlint:main:commitlint": "commitlint",
		"ci:pr:lint:main:eslint": "eslint .",
		"prepare": "husky",
		"vitepress": "bash ./.vitepress/scripts/pre-vitepress.sh && vitepress dev .vitepress",
		"vitepress:preview": "vitepress preview .vitepress"
	},
	"devDependencies": {
		"@angular/build": "^21.1.1",
		"@angular/cli": "^21.1.1",
		"@angular/compiler-cli": "^21.1.1",
		"@commitlint/cli": "^20.3.1",
		"@commitlint/config-conventional": "^20.3.1",
		"@commitlint/prompt-cli": "^20.3.1",
		"@eslint/js": "^9.39.2",
		"@eslint/json": "^0.14.0",
		"@eslint/markdown": "^7.5.1",
		"@ogs-gmbh/linter": "^2",
		"@ogs-gmbh/vitepress-plugin-sidebar": "^1.0.2",
		"@ogs-gmbh/vitepress-theme": "^1.1.0",
		"eslint": "^9.39.2",
		"eslint-plugin-jsdoc": "^62.4.1",
		"eslint-plugin-security": "^3.0.1",
		"eslint-plugin-unicorn": "^56.0.1",
		"@stylistic/eslint-plugin": "^5.8.0",
		"angular-eslint": "^21.2.0",
		"globals": "^17.1.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"ng-packagr": "^21.1.0",
		"npm-package-json-lint": "^9.1.0",
		"stylelint": "^17.0.0",
		"stylelint-scss": "^7.0.0",
		"typedoc": "^0.28.16",
		"typedoc-plugin-frontmatter": "^1.3.1",
		"typedoc-plugin-markdown": "^4.9.0",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.53.1",
		"vitepress": "^1.6.4",
		"vitepress-plugin-group-icons": "^1.7.1",
		"tslib": "^2"
	},
	"peerDependencies": {
		"@angular/common": "^21",
		"@angular/compiler": "^21",
		"@angular/core": "^21",
		"@angular/forms": "^21",
		"@angular/platform-browser": "^21",
		"@angular/router": "^21",
		"@angular/cdk": "^21",
		"@angular/animations": "^21",
		"@angular/material": "^21",
		"@fortawesome/fontawesome-common-types": "^7",
		"@fortawesome/angular-fontawesome": "^4",
		"@fortawesome/fontawesome-svg-core": "^7",
		"@ogs-gmbh/ngx-utils": "^2",
		"rxjs": "~7.8"
	},
	"sideEffects": false,
	"publishConfig": {
		"directory": "dist/main"
	}
}
